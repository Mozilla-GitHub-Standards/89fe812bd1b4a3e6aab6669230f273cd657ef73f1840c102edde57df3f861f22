#check-in
    .venue
        border-bottom 1px solid #8a8a8a
        color #001017
        display block
        font-size 24px
        line-height 33px
        min-height 66px

        p
            font-size 32px
            line-height 42px

.checkin
    border 1px solid gray
    clear both
    margin 5px auto
    max-width 90%
    padding 5px

    &:after
        clear both
        content ""
        display block

    .profile-photo
        height 50px
        margin 0 5px 0 0
        width 50px

    .venue
        overflow hidden
        text-overflow ellipsis
        white-space nowrap
